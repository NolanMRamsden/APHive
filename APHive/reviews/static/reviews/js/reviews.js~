$("a[class^='use']").click(function () {
    console.log('clicked');
    $(this).siblings().removeClass('review-underline');
    $(this).toggleClass('review-underline');
 });

$("a[class^='docs']").click(function () {
    console.log('clicked');
    $(this).siblings().removeClass('review-underline');
    $(this).toggleClass('review-underline');
 });

$("a[class^='rel']").click(function () {
    console.log('clicked');
    $(this).siblings().removeClass('review-underline');
    $(this).toggleClass('review-underline');
 });
